
 <% @page.each do |page_item| %>
<!-- jumbotron of image/slide -->
    <div class="jumbotron">
      <div class="row">
        <div class="col-sm-12 col-md-6">
          <div class="image-responsive">
            <img src="<%= page_item.page_image_url %>" alt="<%= page_item.page_image_caption %>">
            <div><%= page_item.page_image_caption %></div>
          </div>
          <p></p>
          <p> + <%= page_item.page_bullet_01 %></p>
          <p> + <%= page_item.page_bullet_02 %></p>
        </div>
      </div>
    </div>
 <% end %>
 </div>


